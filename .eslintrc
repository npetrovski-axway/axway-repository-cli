{
	"extends": [ "axway/env-node", "axway/+babel" ],
	"globals": {
		"appcd": false
	},
	"rules": {
		"node/no-unsupported-features/es-syntax": "off",
		"node/no-unsupported-features/node-builtins": [ "warn", { "version": ">=10.0.0" } ],
		"require-atomic-updates": "off",
		"indent": ["error", 4],
		"quotes": ["error", "double"]
	}
}